{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "express typescript boilerplate for API",
  "main": "src/server.ts",
  "scripts": {
    "dev": "node src/server.ts",
    "start": "tsc && node dist/server.js",
    "lint": "eslint . --ext .ts",
    "build": "rm -rf build/ && prettier --write source/ && tsc",
    "build:dev": "rimraf ./build && tsc",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "keywords": [
    "express",
    "typescript",
    "rest-api",
    "api",
    "ts"
  ],
  "author": "prashant sudeep",
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "aws-sdk": "^2.1048.0",
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.13"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "chai": "^4.3.4",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.5",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
